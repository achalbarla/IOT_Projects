#include <WiFi.h>

 char* ssid = "Wifi SSID name";
 char* password= "My password";
void setup(){
    Serial.begin(115200);
    WiFi.disconnect();
    delay(300);

    // We start by connecting to a WiFi network
    //Make sure serial monitor is open and correct baud rate is selected or it will print boxes

    Serial.println("Program Start");
       Serial.print("Connecting to.............. ");
    Serial.println(ssid);

    WiFi.begin(ssid, password);

    while (WiFi.status() != WL_CONNECTED) {
        delay(500);
        Serial.print("is it connected.....");
    

    Serial.println("IP address: ");
    Serial.println(WiFi.localIP());
}
}

void loop () {}


